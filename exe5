function somaMatriz(matriz) {
    let somaTotal = 0;
    for (let i = 0; i < matriz.length; i++) {
      for (let j = 0; j < matriz[i].length; j++) {
        somaTotal += matriz[i][j];
      }
    }
    return somaTotal;
  }
const minhaMatriz = [
    [9, 2, 2],
    [4, 7, 0],
    [1, 8, 3]
  ];
  const resultado = somaMatriz(minhaMatriz);
  console.log("A soma dos elementos da matriz Ã©:", resultado);
